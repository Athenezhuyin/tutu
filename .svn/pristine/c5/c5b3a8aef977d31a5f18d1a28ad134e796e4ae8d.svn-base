}

function editsave(formID,actionURL){
	ajaxSubmit(formID,actionURL);
}

function ajaxSubmit(formID,actionURL){
	
	$("#"+formID+"Form").form('submit', {
		url : actionURL,
		onSubmit : function() {
			return true;
		},
		type : "POST", // 设置请求类型为"POST"，默认为"GET"
		success : function() {					
				alert("添加成功！");
				clr();
				$("#"+formID+"win").window("close");
				$("#tList").datagrid('reload');
		}
	});
}
