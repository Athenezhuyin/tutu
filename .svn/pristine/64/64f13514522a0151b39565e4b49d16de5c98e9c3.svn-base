			<div title="旅行社管理">
				<li><a href="travel/add.html" target="iframe">添加人员</a></li>
			</div>
		
		</div>
    </div>
    
  
	<!-- 主页面 -->
    <div id="mainPanle" region="center" style="background: #eee; overflow-y:hidden">
